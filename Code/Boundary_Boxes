public class BoundaryBox {
  float x, y;
  int wd, ht;
  
  BoundaryBox(float x, float y, int wd, int ht) {
    this.x = x;
    this.y = y;
    this.wd = wd;
    this.ht = ht;
  }
  
  float getX() {
    return x;
  }
  
  float getY() {
    return y;
  }
  
  int getWd() {
    return wd;
  }
  
  int getHt() {
    return ht;
  }
}
